          $ref: '#/components/responses/InternalErrorType'
      operationId: getSubjects
      summary: List all Subjects
      description: Gets a list of all `Subject` entities.
    post:
      requestBody:
        description: >-
          The new `Subject` to be created. The subjectId is read-only field and
          must not be provided in input. The subjectId is generated by Neos and
          it is returned in the output.
        content:
          application/json:
            schema:
              $ref: '#/components/schemas/SubjectType'
        required: true
      tags:
        - Subject
      responses:
        '201':
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/SubjectType'
          links:
