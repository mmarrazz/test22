    TransactionType:
      title: Transaction
      description: A single transaction made within an account
      required:
        - transactionId
        - amount
        - currency
      type: object
      properties:
        transactionId:
          $ref: '#/components/schemas/IdType'
        status:
          type: string
          enum:
            - booked
            - pending
        bookingDate:
          description: >-
            The date when the transaction is posted to the account on the bank's
            books
          format: date
          type: string
        valueDate:
          description: >-
            The date at which assets become available to the account owner in
            case of a credit
          format: date
          type: string
        amount:
          format: double
          type: number
        currency:
          $ref: '#/components/schemas/CurrencyCodeType'
        description:
          description: >-
            The reason for the payment, the remittance information in free text
            format
          type: string
          maxLength: 255
        category:
          description: >-
            The category assigned to the transaction by the algorithm for
            automatic categorization
          oneOf:
            - $ref: '#/components/schemas/ConsumerCategoriesType'
            - $ref: '#/components/schemas/BusinessCategoriesType'
        debtorName:
          description: The name of the debtor
          type: string
          maxLength: 80
        debtorAccountIban:
          $ref: '#/components/schemas/IbanType'
        creditorName:
          description: The name of the creditor
          type: string
          maxLength: 80
        creditorAccountIban:
          $ref: '#/components/schemas/IbanType'
      example:
        transactionId: 95
        status: booked
        bookingDate: 2018-01-17T00:00:00.000Z
        amount: 11.78
        currency: EUR
        description: this is the description of the transaction
        category: FA_01
    TransactionArrayType:
      title: Transaction Array
      description: Array of transactions with metadata for pagination
      allOf:
        - $ref: '#/components/schemas/MetadataType'
        - type: object
          properties:
            content:
              type: array
              items:
                $ref: '#/components/schemas/TransactionType'
          example:
            content:
              - transactionId: 95
                status: booked
                bookingDate: 2018-01-17T00:00:00.000Z
                amount: 11.78
                currency: EUR
                description: this is the description of the transaction
                category: FA_01
              - transactionId: 95
                status: booked
                bookingDate: 2018-01-16T00:00:00.000Z
                amount: 1121.02
                currency: EUR
                description: this is the description of the transaction
                category: FA_01
    RecurringIncomeType:
      title: Recurring Income
      description: >-
        List of positive transactions recurring at least once a month in the
        last 3 solar months. The entries are order by `averageMonthlyIncome`
        descending. It is not returned in case the preconditions for calculation
        are not satisfied or there are not incomes identifiable as recurring in
        the last 3 solar months.


        Precondition = transactions available in the last three solar months
      type: object
      properties:
        descriptionSource:
          description: Description of the recurring income
          type: string
          maxLength: 255
        averageMonthlyIncome:
          description: Monthly average of the recurring income
          format: double
          type: number
        matchedTransactions:
          description: >-
            List of transactions identified as occurrences of the recurring
            income
          type: array
          items:
            $ref: '#/components/schemas/IdType'
      example:
        descriptionSource: apartment rent
        averageMonthlyIncome: 500
        matchedTransactions:
          - 6
          - 25
    SubjectVerificationType:
      title: Subject Verification
      description: >-
        The 4 entries are ordered by name, in the following order: 

        1. FirstName

        2. LastName

        3. FirstNameLastName

        4. LaststNameFirstName


        The object SubjectVerification is not returned in case the preconditions
        for calculation are not satisfied.


        Precondition = at lest one transaction available. At least one subject
        name (first name or last name) available
      type: array
      minItems: 4
      maxItems: 4
      items:
        type: object
        properties:
          name:
            type: string
            description: Field for the name attributes
            maxLength: 255
          flagVerification:
            type: boolean
            description: >-
              Whether the name has been found in the description of the last 12
              months incoming transactions
      example:
        - name: MARIO
          flagVerification: true
        - name: ROSSI
          flagVerification: true
        - name: MARIO ROSSI
          flagVerification: false
        - name: ROSSI MARIO
          flagVerification: false
    KpiNumberReverseDebtLowBalanceLast30Type:
      description: >-
        This KPI is calculated only for account in german language.


        Number of transactions, in the indicated timeframe, identified as
        reverse debt caused by balance too low; i.e. when the account balance is
        not enough for the direct debits to be debited


        Precondition = at least one transaction 
      type: integer
      format: int32
