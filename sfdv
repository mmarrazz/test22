              $ref: '#/components/links/DeleteSubjectById'
            GetSubjectKpiById:
              $ref: '#/components/links/GetSubjectKpiById'
            CreateWidgetConnect:
              $ref: '#/components/links/CreateWidgetConnect'
          description: Successful response
        '400':
          $ref: '#/components/responses/BadRequestType'
        '500':
          $ref: '#/components/responses/InternalErrorType'
      operationId: createSubject
      summary: Create a Subject
      description: >-
        Creates a new instance of a `Subject`.


        **Use case:** a new subject that want to create a connection with one
        provider for the first time. The client must save the output subjectId
        in its own system. This is needed in order to get subject data or use
        the same subject again in future calls.
    parameters:
      - $ref: '#/components/parameters/requestIdParam'
