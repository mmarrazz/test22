      description: Zero-based number of the page to obtain
      schema:
        default: 0
        minimum: 0
        type: integer
        format: int64
      in: query
      required: false
    sizeParam:
      name: size
      description: Maximum number of elements inside a page
      schema:
        default: 20
        maximum: 100
        minimum: 1
        type: integer
        format: int64
      in: query
      required: false
    sortParam:
      name: sort
      description: >-
        sorting strategy in the format `field(,asc|desc)`. Default sort order is
