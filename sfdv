        '500':
          $ref: '#/components/responses/InternalErrorType'
      operationId: getSubjectKpi
      summary: Get the KPIs of a Subject
      description: >-
        Returns a `ConsumerKpi` or `BusinessKpi` or both, according to the
        purpose of the account(s) linked to the subject. 


        The KPI are returned the in the requested currency. Default to EUR. In
        case the requested currency is different from EUR, the KPI are converted
        using the most recent exchange rate available in Neos.
    parameters:
      - $ref: '#/components/parameters/subjectIdParam'
      - $ref: '#/components/parameters/requestIdParam'
      - $ref: '#/components/parameters/currencyCodeParam'
