          'https://failure.url/defined/during/onboarding':
            post:
              operationId: postFailureCallback
              summary: Callback used in case of failure
              requestBody:
                required: true
                content:
                  application/json:
                    schema:
                      type: object
                      required:
                        - connectionId
                      properties:
                        connectionId:
                          $ref: '#/components/schemas/IdType'
                        callbackFields:
                          $ref: '#/components/schemas/CallbackFieldsType'
                        message:
                          type: string
                      example:
                        connectionId: 345
                        message: connection timeout error XYZ
              responses:
                '200':
                  $ref: '#/components/responses/SuccessNoContentType'
